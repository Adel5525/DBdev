-- -----------------------------------------------------
-- Table `Huertos`.`Доставка`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `Huertos`.`Доставка` (
  `id dostavki` INT NOT NULL,
  `price dostavki` VARCHAR(45) NULL,
  `sroki` DATETIME NULL,
  `Компания_id kompany` INT NOT NULL,
  PRIMARY KEY (`id dostavki`, `Компания_id kompany`),
  INDEX `fk_Доставка_Компания1_idx` (`Компания_id kompany` ASC) VISIBLE,
  CONSTRAINT `fk_Доставка_Компания1`
    FOREIGN KEY (`Компания_id kompany`)
