-- -----------------------------------------------------
-- Table `Huertos`.`Струдничество`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `Huertos`.`Струдничество` (
  `id labaratory` INT NOT NULL,
  `name` VARCHAR(45) NULL,
  `Компания_id kompany` INT NOT NULL,
  PRIMARY KEY (`id labaratory`, `Компания_id kompany`),
  INDEX `fk_С кем работают_Компания1_idx` (`Компания_id kompany` ASC) VISIBLE,
