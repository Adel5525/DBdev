-- -----------------------------------------------------
-- Schema Huertos
-- -----------------------------------------------------
CREATE SCHEMA IF NOT EXISTS `Huertos` DEFAULT CHARACTER SET utf8 ;
USE `Huertos` ;

-- -----------------------------------------------------
-- Table `Huertos`.`Компания`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `Huertos`.`Компания` (
  `id kompany` INT NOT NULL,
  `Name` VARCHAR(25) NULL,
  PRIMARY KEY (`id kompany`),
  UNIQUE INDEX `id kompany_UNIQUE` (`id kompany` ASC) VISIBLE)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `Huertos`.`Адрес`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `Huertos`.`Адрес` (
  `id adress` INT NOT NULL,
  `adress` VARCHAR(45) NULL,
  `Компания_id kompany` INT NOT NULL,
  PRIMARY KEY (`id adress`, `Компания_id kompany`),
  INDEX `fk_Адрес_Компания_idx` (`Компания_id kompany` ASC) VISIBLE,
  CONSTRAINT `fk_Адрес_Компания`
    FOREIGN KEY (`Компания_id kompany`)
    REFERENCES `Huertos`.`Компания` (`id kompany`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `Huertos`.`Контакты`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `Huertos`.`Контакты` (
  `id con` INT NOT NULL,
  `phone` VARCHAR(25) NULL,
  `email` VARCHAR(25) NULL,
  `Компания_id kompany` INT NOT NULL,
  PRIMARY KEY (`id con`, `Компания_id kompany`),
  INDEX `fk_Контакты_Компания1_idx` (`Компания_id kompany` ASC) VISIBLE,
  CONSTRAINT `fk_Контакты_Компания1`
    FOREIGN KEY (`Компания_id kompany`)
    REFERENCES `Huertos`.`Компания` (`id kompany`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `Huertos`.`Цена`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `Huertos`.`Цена` (
  `id price` INT NOT NULL,
  `price printer` VARCHAR(45) NULL,
  `price fotopolimer` VARCHAR(45) NULL,
  `Компания_id kompany` INT NOT NULL,
  PRIMARY KEY (`id price`, `Компания_id kompany`),
  INDEX `fk_Ценовой сегмент_Компания1_idx` (`Компания_id kompany` ASC) VISIBLE,
  CONSTRAINT `fk_Ценовой сегмент_Компания1`
    FOREIGN KEY (`Компания_id kompany`)
    REFERENCES `Huertos`.`Компания` (`id kompany`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `Huertos`.`Бренды`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `Huertos`.`Бренды` (
  `id brend` INT NOT NULL,
  `name` VARCHAR(45) NULL,
  `Компания_id kompany` INT NOT NULL,
  PRIMARY KEY (`id brend`, `Компания_id kompany`),
  INDEX `fk_Бренды_Компания1_idx` (`Компания_id kompany` ASC) VISIBLE,
  CONSTRAINT `fk_Бренды_Компания1`
    FOREIGN KEY (`Компания_id kompany`)
    REFERENCES `Huertos`.`Компания` (`id kompany`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `Huertos`.`Сервис`
-- -----------------------------------------------------
